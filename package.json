{
    "name": "space-data-server",
    "version": "23.3.3-0.1.1-1.7.0001+1679612934891",
    "description": "mvp for standards-based space data server",
    "main": "index.ts",
    "standardsPath": "./lib/class/standards",
    "files": [
        "build",
        "lib/class",
        "go-ipfs",
        "swagger-output.json"
    ],
    "directories": {},
    "scripts": {
        "deploy": "npm run build:ui && npm run build:swagger && npm run build && git add -A && git commit -m 'update build' && git push && git push origin --tags && npm publish",
        "init": "git submodule init && git submodule update && npm run build:standards",
        "build": "npm run build:standards && npm run build:celestrak:parsers && cp -R ./node_modules/better-sqlite3/build/Release/* build/ && npm run build:esbuild && node ./scripts/updateVersion.mjs",
        "build:cli": "esbuild cli.ts --bundle --platform=node --outfile=./build/cli.cjs",
        "build:ui": "cd UI && npm run build",
        "build:esbuild": "esbuild index.ts --sourcemap --external:pg-native --external:better_sqlite3 --bundle --platform=node --external:fsevents --outfile=./build/server.cjs",
        "build:celestrak:parsers": "npm run build:celestrak:parser:omm && npm run build:celestrak:parser:cat",
        "build:celestrak:parser:omm": "esbuild ./scripts/data_fetch/celestrak/src/celestrak.omm.ts --bundle --platform=node --outfile=./scripts/data_fetch/celestrak/celestrak.omm.cjs",
        "build:celestrak:parser:cat": "esbuild ./scripts/data_fetch/celestrak/src/celestrak.cat.ts --bundle --platform=node --outfile=./scripts/data_fetch/celestrak/celestrak.cat.cjs",
        "build:standards": "cd packages/spacedatastandards.org && git reset --hard && git fetch && git checkout v23.3.3-0.2.1 && cd ../../ && node ./lib/utility/generator.mjs",
        "build:swagger": "node ./scripts/swagger.mjs",
        "build:docker": "docker build -t space-data-server .",
        "start": "node --max-old-space-size=4096 ./build/server.cjs",
        "dev": "rm -rf public/* && npm run dev:clean:database && npm run build && PORT=8080 nodemon",
        "dev:clean:database": "rm -rf .database/*",
        "dev:database": "cp ./lib/config/default.config.database.json config.json && npm run dev:clean:database && PORT=8080 nodemon --config nodemon.database.json",
        "dev:standards": "nodemon --config nodemon.generator.json",
        "dev:parsers": "nodemon --config nodemon.parser.json",
        "dev:cli": "nodemon --config nodemon.cli.json",
        "test:cli": "jest --watchAll --detectOpenHandles --runInBand ./test/cli.test",
        "test:gencert": "cd test/certs/ && chmod +x generatecerts.sh && ./generatecerts.sh",
        "test:server:cluster": "export IPFS_PATH=$PWD/go-ipfs/ && cp ./test/test.config.json config.json && npm run build:standards && jest --watchAll --detectOpenHandles --runInBand ./test/server.cluster.test",
        "test:server:database": "export IPFS_PATH=$PWD/go-ipfs/ && cp ./test/test.config.database.json config.json && npm run build:standards && jest --watchAll --detectOpenHandles --runInBand ./test/server.database.test",
        "test:server:filesystem": "export IPFS_PATH=$PWD/go-ipfs/ && cp ./test/test.config.filesystem.json config.json && npm run build:standards && jest --watchAll --detectOpenHandles --runInBand ./test/server.filesystem.test",
        "test:admin": "export IPFS_PATH=$PWD/go-ipfs/ && cp ./test/test.config.admin.json config.json && npm run build:standards && jest --watchAll --detectOpenHandles --runInBand ./test/admin.test",
        "test:parser": "export IPFS_PATH=$PWD/go-ipfs/ && cp ./test/test.config.json config.json && jest --watchAll --detectOpenHandles --runInBand ./test/parser.test",
        "test:ipfs": "export IPFS_PATH=$PWD/go-ipfs/ && jest --watchAll --detectOpenHandles --runInBand ./test/ipfs.test",
        "test:smartcontract": "nodemon --config nodemon.smartcontract.json"
    },
