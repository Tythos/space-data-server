{
    "name": "space-data-server",
    "version": "1.0.1355+1676400215170",
    "description": "mvp for standards-based space data server",
    "main": "index.ts",
    "standardsPath": "./lib/class/standards",
    "scripts": {
        "deploy": "npm run build:ui && npm run build:swagger && npm run build && cd UI && npm run build && cd .. && git add -A && git commit -m 'update build' && git push && npm publish",
        "init": "git submodule init && git submodule update && npm run build:standards",
        "build": "npm run build:standards && npm run build:celestrak:parsers && cp -R ./node_modules/better-sqlite3/build/Release/* build/ && npm run build:esbuild && node ./scripts/updateVersion.mjs",
        "build:ui": "cd UI && npm run build",
        "build:esbuild": "esbuild index.ts --sourcemap --external:pg-native --external:better_sqlite3 --bundle --platform=node --external:fsevents --outfile=./build/server.cjs",
        "build:celestrak:parsers": "esbuild ./scripts/data_fetch/celestrak/src/celestrak.omm.ts --bundle --platform=node --outfile=./scripts/data_fetch/celestrak/celestrak.omm.cjs",
        "build:standards": "cd packages/spacedatastandards.org && git reset --hard && git checkout main && git pull && cd ../../ && node ./lib/utility/generator.mjs",
        "build:swagger": "node ./scripts/swagger.mjs",
        "start": "npm run build && node ./build/server.cjs",
        "dev": "rm -rf config.json && npm run build && PORT=8080 nodemon",
        "dev:standards": "nodemon --config nodemon.generator.json",
        "dev:parsers": "nodemon --config nodemon.parser.json",
        "test": "npm run build:standards && jest --watchAll --detectOpenHandles --runInBand ./test/core.test",
        "test:server": "cp ./test/test.config.json config.json && npm run build:standards && jest --watchAll --detectOpenHandles --runInBand ./test/server.test",
        "test:parser": "jest --watchAll --detectOpenHandles --runInBand ./test/parser.test"
    },
    "keywords": [],
    "author": "",
    "license": "ISC",
