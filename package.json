{
    "name": "space-data-server",
    "version": "1.0.6+1669664996947",
    "description": "mvp for standards-based space data server",
    "main": "index.ts",
    "standardsPath": "./lib/class/standards",
    "scripts": {
        "deploy": "npm run build && git add -A && git commit -m 'update build' && git push && npm publish",
        "postinstall": "git submodule init && git submodule update && npm run build:standards",
        "build": "cp -R ./node_modules/better-sqlite3/build/Release/* build/ && npm run build:esbuild && node ./scripts/updateVersion.mjs",
        "build:esbuild": "esbuild index.ts --external:pg-native --external:better_sqlite3 --bundle --platform=node > ./build/server.cjs",
        "build:standards": "cd packages/spacedatastandards.org && git reset --hard && git checkout main && git pull && cd ../../ && node ./lib/utility/generator.mjs",
        "start": "npm run build && node ./build/server.cjs",
        "dev": "npm run build && PORT=3000 nodemon",
        "dev:standards": "nodemon --config nodemon.generator.json",
        "test": "npm run build:standards && jest --watchAll --detectOpenHandles --runInBand"
    },
    "keywords": [],
    "author": "",
    "license": "ISC",
    "dependencies": {},
    "devDependencies": {
        "@noble/secp256k1": "^1.7.0",
        "@types/json-schema": "^7.0.11",
        "aws-sdk": "^2.1254.0",
        "better-sqlite3": "^7.6.2",
        "bip32": "^3.1.0",
        "bip39": "^3.0.4",
        "bitcoinjs-lib": "^6.0.2",
        "bitcoinjs-message": "^2.2.0",
        "body-parser": "^1.20.0",
        "cors": "^2.8.5",
        "dotenv": "^16.0.2",
        "ecpair": "^2.1.0",
        "ethers": "^5.7.2",
        "ethers-ens": "^0.2.1",
        "express": "^4.18.2",
        "flatbuffers": "22.10.26",
        "helmet": "^6.0.0",
        "ipfs-core": "^0.17.0",
        "jose": "^4.9.3",
        "json2csv": "^5.0.7",
        "knex": "^2.3.0",
        "mock-aws-s3": "^4.0.2",
        "mysql": "^2.18.1",
        "mysql2": "^2.3.3",
        "nock": "^13.2.9",
        "oracledb": "^5.5.0",
        "pg": "^8.8.0",
        "pg-query-stream": "^4.2.4",
