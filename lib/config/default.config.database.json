{
  "server": {
    "port": 8080,
    "key": "",
    "cert": ""
  },
  "nonceTimeout": 30000,
  "data": {
    "ingest": ".data",
    "fileSystemPath": "public",
    "useDatabase": true,
    "cache": "5 minutes",
    "verbose": false,
    "defaultLimit": 100,
    "limit": {
      "CAT": 2
    }
  },
  "trustedAddresses": [],
  "database": {
    "path": ".database",
    "limits": {
      "totalRecords": 500
    },
    "config": {
      "primary": "sqlite",
      "sqlite": {
        "client": "better-sqlite3",
        "connection": {
          "filename": "./.database/spacedatastandards.0.0.2+1675110065585.sqlite"
        },
        "useNullAsDefault": true,
        "version": "0.0.2+1675110065585"
      },
      "pg": {
        "client": "pg",
        "connection": "postgresql://test@localhost/standards",
        "pool": {
          "min": 2,
          "max": 10
        },
        "migrations": {
          "tableName": "knex_migrations"
        }
      }
    }
  }
}
